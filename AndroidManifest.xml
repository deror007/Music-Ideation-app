<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.musicideation">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MusicIdeation">
        <service android:name="com.example.musicideation.SoundService"/>
        <activity android:name=".SelectNotation" />
        <activity android:name=".ViewNotations" />
        <activity android:name=".UpdateSongProperties" />
        <activity android:name=".SongProperties" />
        <activity android:name=".SongIdeas" />
        <activity android:name=".UserGuide" />
        <activity android:name=".DrawNotation" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>